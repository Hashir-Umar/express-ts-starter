{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": ["prettier", "airbnb-base"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "indent": "off",
        "consistent-return": "warn",
        "global-require": "off",
        "camelcase": "warn",
        "comma-dangle": ["error", "always-multiline"],
        "class-methods-use-this": "off"
    }
}